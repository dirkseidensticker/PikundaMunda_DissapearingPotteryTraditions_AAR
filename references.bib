@article{Cordell.1993,
  title = {Chronological Variability in Ceramic Paste: {{A}} Comparison of Deptford and Savannah Period Pottery in the St. {{Marys}} River Region of Northeast {{Florida}} and Southeast {{Georgia}}},
  author = {Cordell, Ann S.},
  year = {1993},
  journal = {Southeastern Archaeology},
  volume = {12},
  number = {1},
  eprint = {40712986},
  eprinttype = {jstor},
  pages = {33--58},
  publisher = {{[Maney Publishing, Southeastern Archaeological Conference]}},
  issn = {0734578X},
  urldate = {2021-07-14},
  abstract = {[Paste and technological variability of a sample of Deptford (500 B.C.-A. D. 800) and Savannah (A.D. 800-1500) period pottery from the St. Marys region were investigated through microscopic examination of aplastic constituents together with consideration ofrefired paste color and sherd thickness. Three gross paste categories were recognized: non-spiculateI non-micaceous paste, representing at least two to five possible clay sources; sponge-spiculate paste representing at least four to five clay sources; and micaceous paste, representing at least four clay sources. It is necessary to use a microscope with at least 105 x in order to recognize most of the paste categories. The results show that Deptford and Savannah period pottery can be distinguished in terms of paste as well as surface treatment. The Deptford period samples are characterized by the predominance of nonspiculateI non-micaceous grit and sand paste. The Savannah period samples are characterized by the abundance of spongespiculate and micaceous pastes. Plain and cord-marked Savannah period pottery is further distinguished by the prominence of grog tempering and absence of grit tempering in the plain wares. The results indicate that these data may be useful for predicting the temporal affiliation of St. Marys region pottery assemblages that lack a clear-cut distinction in surface treatment or in the absence of diagnostic decorated types or other datable remains. The paste data are also well suited for studying the pottery in terms of patterns of clay resource use and manufacturing origins.]},
  keywords = {Amerika/Nord-,Sponge spicules},
  file = {C\:\\Users\\dirks\\OneDrive\\Literatur\\Cordell_1993_Chronological variability in ceramic paste\\Cordell - 1993 - Chronological variability in ceramic paste A comp.pdf}
}

@article{Costa.2004,
  title = {The Ceramic Artifacts in Archaeological Black Earth (Terra Preta) from Lower {{Amazon}} Region, {{Brazil}}: Mineralogy},
  shorttitle = {The Ceramic Artifacts in Archaeological Black Earth (Terra Preta) from Lower {{Amazon}} Region, {{Brazil}}},
  author = {da Costa, Marcondes Lima and Kern, Dirse Clara and Pinto, Alice Helena Eleot{\'e}rio and Souza, Jorge Raimundo da Trindade},
  year = {2004},
  journal = {Acta Amazonica},
  volume = {34},
  number = {2},
  pages = {165--178},
  issn = {0044-5967},
  doi = {10.1590/S0044-59672004000200004},
  urldate = {2021-07-14},
  abstract = {Several archaeological black earth (ABE) sites occur in the Amazon region. They contain fragments of ceramic artifacts, which are very important for the archaeological purpose. In order to improve the archaeological study in the region we carried out a detailed mineralogical and chemical study of the fragments of ceramic artifacts found in the two ABE sites of CachoeiraPorteira, in the Lower Amazon Region. Their ceramics comprise the following tempers: cauixi, cariap\'e, sand, sand +feldspars, crushed ceramic and so on and are composed of quartz, clay equivalent material (mainly burned kaolinite), feldspars, hematite, goethite, maghemite, phosphates, anatase, and minerals of Mn and Ba. Cauixi and cariap\'e, siliceous organic compounds, were found too. The mineralogical composition and the morphology of their grains indicate a saprolite (clayey material rich on quartz) derived from fine-grained felsic igneous rocks or sedimentary rocks as source material for ceramic artifacts, where silica-rich components such cauixi, cariap\'e and/or sand (feldspar and rock fragments) were intentionally added to them. The high content of (Al,Fe)-phosphates, amorphous to low crystalline, must be product of the contact between the clayey matrix of pottery wall and the hot aqueous solution formed during the daily cooking of animal foods (main source of phosphor). The phosphate crystallization took place during the discharge of the potteries put together with waste of organic material from animal and vegetal origin, and leaving to the formation of the ABE-soil profile.},
  langid = {english},
  keywords = {Amerika/S\"ud-,Sponge spicules},
  file = {C\:\\Users\\dirks\\OneDrive\\Literatur\\Costa et al_2004_The ceramic\\Costa et al_2004_The ceramic.pdf}
}

