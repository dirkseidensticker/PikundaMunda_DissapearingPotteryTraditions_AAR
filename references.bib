  title = {{Petrografia de cer\^amicas da fase Bacabal (sambaqui Monte Castelo): um dos mais antigos usos de cauixi na Amaz\^onia}},
  shorttitle = {{Petrografia de cer\^amicas da fase Bacabal (sambaqui Monte Castelo)}},
  author = {Villagran, Ximena Suarez and Alves, Marcony Lopes and Kater, Thiago and da Silva, Kelly Brand{\~a}o Vaz and Batalla, Nicol{\'a}s and da Costa, Marcelo and Costa, Jessica and Erler, Dionne Miranda Azevedo and Ferreira, Mariane Pereira and Magon, Patr{\'i}cia and dos Reys, Aranda Cali{\'o} and Straioto, Haruan and Tognoli, Anderson Rog{\'e}rio and Strauss, Andr{\'e} and del Lama, Eliane Aparecida and Martins, Lucelene and {Teixeira-Bastos}, Marcio and Neves, Eduardo G{\'o}es and Zimpel, Carlos and Pugliese, Francisco},
  year = {2022},
